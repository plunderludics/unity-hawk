{
  "name": "org.plunderludics.unityhawk",
  "version": "0.0.4",
  "displayName": "UnityHawk",
  "description": "Run emulated games within Unity via BizHawk",
  "dependencies": {
    "com.unity.nuget.newtonsoft-json": "3.0.2",
    "com.dbrizov.naughtyattributes": "2.1.4"
  },
  "documentationUrl": "https://example.com/",
  "changelogUrl": "https://example.com/changelog.html",
  "licensesUrl": "https://example.com/licensing.html",
  "keywords": [
    "plunderludics",
    "emulation",
    "bizhawk"
  ],
  "author": {
    "name": "plunderludics group",
    "url": "https://plunderludics.github.io"
  },
  "scopedRegistries": [
    {
      "name": "package.openupm.com",
      "url": "https://package.openupm.com",
      "scopes": [
        "com.dbrizov.naughtyattributes",
        "com.openupm"
      ]
    }
  ],
  "samples": [
    {
        "displayName": "Demo",
        "description": "Demo scene with emulator on tv. (Copy contents of Demo/StreamingAssets/ into the root Assets/StreamingAssets directory to make this work)",
        "path": "Samples~/Demo"
    }
  ]
}